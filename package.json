{
  "name": "console-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run lint --fix && vue-cli-service build",
    "build:dev": "npm run lint --fix && vue-cli-service build --mode dev",
    "build:pre": "npm run lint --fix && vue-cli-service build --mode pre",
    "build:test": "npm run lint --fix && vue-cli-service build --mode test",
    "lint": "vue-cli-service lint",
    "dev": "npm start",
    "start": "npm run lint --fix && vue-cli-service serve",
    "svg": "svgo -f src/themes/icons/svg --config=src/themes/icons/svgo.yml",
    "create": "plop"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "crypto-js": "^3.1.9-1",
    "element-ui": "^2.12.0",
    "fuse.js": "^3.4.5",
    "js-file-download": "^0.4.8",
    "jsencrypt": "^3.0.0-rc.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^3.0.0",
    "screenfull": "^4.2.0",
    "vue": "^2.6.6",
    "vue-i18n": "^8.11.2",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "wangeditor": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.2",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^7.2.2",
    "plop": "^2.3.0",
    "postcss-pxtorem": "^4.0.1",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "style-resources-loader": "^1.2.1",
    "svg-sprite-loader": "^4.1.6",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vconsole-webpack-plugin": "^1.4.2",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.34.0",
    "webpack-bundle-analyzer": "^3.1.0"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
